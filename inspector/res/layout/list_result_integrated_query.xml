<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/list_result_integrated_query"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
    <LinearLayout 
    android:id="@+id/list_result_integrated_query"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal" >
  <LinearLayout 
      android:layout_width="0dp"
      android:layout_height="wrap_content"
      android:layout_weight="6"
      android:orientation="vertical" >
      <LinearLayout 
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <TextView 
          android:id="@+id/tv_administrative_division_integrated_query"
          android:layout_width="0dp"
          android:layout_height="wrap_content"
          android:layout_weight="1"
          android:background="#EBEBEB"
          android:paddingLeft="3dp"
          android:paddingRight="3dp"
          android:textSize="12sp"/>
        <TextView 
          android:id="@+id/tv_parking_name_integrated_query"
          android:layout_width="0dp"
          android:layout_height="wrap_content"
          android:layout_weight="1"
          android:background="#EBEBEB"
          android:paddingLeft="3dp"
          android:paddingRight="3dp"
          android:textSize="12sp"/>
        <TextView 
          android:id="@+id/tv_parking_number_integrated_query"
          android:layout_width="0dp"
          android:layout_height="wrap_content"
          android:layout_weight="1"
          android:background="#EBEBEB"
          android:paddingLeft="3dp"
          android:paddingRight="3dp"
          android:textSize="12sp"/>
      </LinearLayout>
      <LinearLayout 
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <TextView 
          android:id="@+id/tv_license_plate_integrated_query"
          android:layout_width="0dp"
          android:layout_height="wrap_content"
          android:layout_weight="1"
          android:background="#EBEBEB"
          android:paddingLeft="3dp"
          android:paddingRight="3dp"
          android:textColor="@color/orange"
          android:textSize="12sp"/>
        <TextView 
          android:id="@+id/tv_toll_collector_name_integrated_query"
          android:layout_width="0dp"
          android:layout_height="wrap_content"
          android:layout_weight="1"
          android:background="#EBEBEB"
          android:paddingLeft="3dp"
          android:paddingRight="3dp"
          android:textSize="12sp"/>
        <TextView 
          android:id="@+id/tv_toll_collector_number_integrated_query"
          android:layout_width="0dp"
          android:layout_height="wrap_content"
          android:layout_weight="1"
          android:background="#EBEBEB"
          android:paddingLeft="3dp"
          android:paddingRight="3dp"
          android:textSize="12sp"/>
      </LinearLayout>
      <LinearLayout 
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <TextView 
          android:id="@+id/tv_car_type_integrated_query"
          android:layout_width="0dp"
          android:layout_height="wrap_content"
          android:layout_weight="1"
          android:background="#EBEBEB"
          android:paddingLeft="3dp"
          android:paddingRight="3dp"
          android:textSize="12sp"/>
        <TextView 
          android:id="@+id/tv_parking_type_integrated_query"
          android:layout_width="0dp"
          android:layout_height="wrap_content"
          android:layout_weight="1"
          android:background="#EBEBEB"
          android:paddingLeft="3dp"
          android:paddingRight="3dp"
          android:textSize="12sp"/>
        <TextView 
          android:id="@+id/tv_location_number_integrated_query"
          android:layout_width="0dp"
          android:layout_height="wrap_content"
          android:layout_weight="1"
          android:background="#EBEBEB"
          android:paddingLeft="3dp"
          android:paddingRight="3dp"
          android:textSize="12sp"/>
      </LinearLayout>
 </LinearLayout>
 <LinearLayout 
      android:layout_width="0dp"
      android:layout_height="match_parent"
      android:layout_weight="1"
      android:gravity="center"
      android:orientation="vertical" >
        <TextView 
            android:id="@+id/tv_expense_integrated_query"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#EBEBEB"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            android:layout_gravity="center_horizontal"
            android:gravity="center"
            android:textSize="12sp"/>
   </LinearLayout>
 </LinearLayout>
 
 <LinearLayout 
    android:id="@+id/list_result_integrated_query"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal" >
       <TextView 
        android:id="@+id/tv_start_time_integrated_query"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:background="#EBEBEB"
        android:paddingLeft="3dp"
        android:paddingRight="3dp"
        android:layout_gravity="center_horizontal"
        android:textSize="12sp"/>
      <TextView 
        android:id="@+id/tv_leave_time_integrated_query"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:background="#EBEBEB"
        android:paddingLeft="3dp"
        android:paddingRight="3dp"
        android:layout_gravity="center_horizontal"
        android:textSize="12sp"/>
 </LinearLayout>
   
</LinearLayout>